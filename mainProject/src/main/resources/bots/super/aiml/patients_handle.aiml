<set var="years">
<interval>
<jformat>MMMMMMMMM dd, yyyy</jformat>
<style>years</style>
<from>March 9, 2022</from>
<to><date jformat="MMMMMMMMM dd, yyyy"/></to>
</interval>
</set>
<set var="months">
<interval>
<jformat>MMMMMMMMM dd, yyyy</jformat>
<style>months</style>
<from>March 9, 2022</from>
<to><date jformat="MMMMMMMMM dd, yyyy"/></to>
</interval>
</set>
</think>
<condition var="years">
<li value="0">I am <get var="months"/> months old.</li>
<li>I am <get var="years"/> years old.</li>
</condition></template>
